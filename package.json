{
  "name": "@gliff-ai/audit",
  "version": "0.1.0",
  "description": "gliff.ai AUDIT",
  "repository": "git://github.com/gliff-ai/audit.git",
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx --no-install vite build -c vite.config.lib.ts && tsc --emitDeclarationOnly --outDir dist && tsc-alias -p tsconfig.json",
    "watch": "npx --no-install vite build -c vite.config.lib.ts --watch",
    "format": "npx --no-install prettier --write src",
    "prettier": "npx --no-install prettier -c src",
    "lint": "npm run lint:ts && npm run prettier",
    "lint:ts": "npx --no-install eslint src",
    "serve": "vite",
    "build:example": "vite build",
    "test": "jest --coverage --passWithNoTests"
  },
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@gliff-ai/annotate": "^0.25.5",
    "@gliff-ai/style": "^0.8.6",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@types/jest": "^26.0.21",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.3",
    "prettier": "=2.3.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.1",
    "tsc-alias": "^1.3.9",
    "typescript": "^4.2.5",
    "vite": "^2.5.3",
    "vite-aliases": "^0.8.6",
    "vite-plugin-checker": "^0.3.4"
  },
  "peerDependencies": {
    "@gliff-ai/annotate": "^0.25.4",
    "@gliff-ai/style": "^0.8.6",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "react": "^17.0.0",
    "react-dom": "^17.0.1"
  },
  "dependencies": {}
}
